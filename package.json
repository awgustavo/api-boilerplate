{
  "name": "api-boilerplate",
  "version": "0.0.1",
  "description": "Api Boiler Plate",
  "main": "server.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/application/server.ts",
    "dev": "npm run code-lint && npm test && ts-node-dev -r tsconfig-paths/register ./src/application/server.ts",
    "test": "jest --coverage",
    "code-lint": "eslint --ext .ts . --fix",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/infrastructure/providers/typeorm/app-data-source.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awgustavo/api-boilerplate.git"
  },
  "keywords": [
    "boiler_plate"
  ],
  "author": "andre_gustavo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/awgustavo/api-boilerplate/issues"
  },
  "homepage": "https://github.com/awgustavo/api-boilerplate#readme",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "jest": "^29.2.1",
    "jest-ts-auto-mock": "^2.1.0",
    "nodemon": "^2.0.20",
    "sucrase": "^3.28.0",
    "ts-auto-mock": "^3.6.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.200.0",
    "@aws-sdk/types": "^3.200.0",
    "@types/express": "^4.17.14",
    "apollo-server": "^3.11.1",
    "aws-sdk": "^2.1244.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-jwt": "^7.7.7",
    "graphql": "^16.6.0",
    "mongodb": "^4.11.0",
    "pg": "^8.8.0",
    "tsyringe": "^4.7.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.10"
  }
}
